package com.javatpoint;

public class Emp extends Person{

	float amount;
	
	Emp(int id, String name, float amount) {
		super(id, name);
		this.amount = amount;
	}
	
	public static void main(String[] args) {
		Emp emp = new Emp(1, "Sai", 4000.0f);
		System.out.println(emp.id+" "+emp.name+" "+emp.amount);
	}
}
